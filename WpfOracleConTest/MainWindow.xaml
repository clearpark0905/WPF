<Window x:Class="WpfOracleConTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfOracleConTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="450">
    <Grid>
        <ListView x:Name="ListView01" HorizontalAlignment="Left" Width="410" Height="400" VerticalAlignment="Top">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Ename" Width="200" DisplayMemberBinding="{Binding Ename}"/>
                    <GridViewColumn Header="Empno" Width="200" DisplayMemberBinding="{Binding Empno}" />
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="btn" Content="Button" HorizontalAlignment="Left" Margin="318,410,0,0" VerticalAlignment="Top" Width="82" Height="35" Click="btn_Click" />
    </Grid>
</Window>
