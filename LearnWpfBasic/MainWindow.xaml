<Window x:Class="LearnWpfBasic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LearnWpfBasic"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <!--<Frame Source="/Welcome.xaml" />-->
    
    <StackPanel>
        <TextBlock Text="TextBlock" />
        <TextBox Text="Textbox" />
        <ProgressBar Value="50" Width="60" Height="20" />
        <Slider Value="5" Width="60" />
        <PasswordBox Password="Secret" />
        <Label>
            <Hyperlink NavigateUri="/Welcome.xaml">Welcome!</Hyperlink>
        </Label>
    </StackPanel>
</Window>
